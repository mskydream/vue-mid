           <v-form\n                \n                ref=\"form\"\n                v-model=\"valid\"\n                lazy-validation\n            >\n                <v-text-field\n                v-model=\"name\"\n                :counter=\"10\"\n                :rules=\"nameRules\"\n                label=\"Name\"\n                required\n                ></v-text-field>\n\n                <v-text-field\n                v-model=\"email\"\n                :rules=\"emailRules\"\n                label=\"E-mail\"\n                required\n                ></v-text-field>\n\n                <v-textarea\n                v-model=\"message\"\n                :rules=\"messageRules\"\n                label=\"Message\"\n                required\n                ></v-textarea>\n\n\n                <v-btn\n                :disabled=\"!valid\"\n                color=\"success\"\n                class=\"mr-4\"\n                @click=\"validate\"\n                >\n                Submit\n                </v-btn>\n\n                <v-btn\n                color=\"error\"\n                class=\"mr-4\"\n                @click=\"reset\"\n                >\n                Reset \n                </v-btn>\n\n            </v-form>\n        </div>\n\n        \n       \n    </div>\n</template>\n<script>\nexport default {\n    data(){\n        return{\n            contacts: [\n                { title: 'About Us', src: 'https://cdn.pixabay.com/photo/2022/01/07/07/13/chicago-6921297_960_720.jpg', flex: 12 },\n            ],\n            valid: true,\n            name: '',\n            nameRules: [\n                v => !!v || 'Name is required',\n                v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n            ],\n            email: '',\n            emailRules: [\n                v => !!v || 'E-mail is required',\n                v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\n            ],\n            message: '',\n            messageRules: [\n                v => !!v || 'Message is required',\n                v => (v && v.length >= 10) || 'Message must be more than 10 characters',\n            ],\n        }\n    },\n    methods: {\n      validate () {\n        this.$refs.form.validate()\n      },\n      reset () {\n        this.$refs.form.reset()\n      },\n    },\n}\n</script>"]}]}