ow dense>\n                <v-col\n                v-for=\"contact in contacts\"\n                :key=\"contact.title\"\n                :cols=\"contact.flex\"\n                >\n                <v-card tile>\n                    <v-img\n                    :src=\"contact.src\"\n                    class=\"white--text align-end\"\n                    gradient=\"to bottom, rgba(0,0,0,.1), rgba(0,0,0,.5)\"\n                    max-height=\"300px\"\n                    >\n                    <v-card-title v-text=\"contact.title\"></v-card-title>\n                    </v-img>\n                </v-card>\n                </v-col>\n            </v-row>\n        </v-container>\n\n        <h1 class=\"text-center display-4 hidden-xs-only my-6\">Your review</h1>\n        <h6 class=\"text-center title hidden-sm-and-up my-6\">Your review</h6>\n\n        <div max-width=\"1080px\" class=\"pa-6\">\n            <v-form\n                \n                ref=\"form\"\n                v-model=\"valid\"\n                lazy-validation\n            >\n                <v-text-field\n                v-model=\"name\"\n                :counter=\"10\"\n                :rules=\"nameRules\"\n                label=\"Name\"\n                required\n                ></v-text-field>\n\n                <v-text-field\n                v-model=\"email\"\n                :rules=\"emailRules\"\n                label=\"E-mail\"\n                required\n                ></v-text-field>\n\n                <v-textarea\n                v-model=\"message\"\n                :rules=\"messageRules\"\n                label=\"Message\"\n                required\n                ></v-textarea>\n\n\n                <v-btn\n                :disabled=\"!valid\"\n                color=\"success\"\n                class=\"mr-4\"\n                @click=\"validate\"\n                >\n                Submit\n                </v-btn>\n\n                <v-btn\n                color=\"error\"\n                class=\"mr-4\"\n                @click=\"reset\"\n                >\n                Reset \n                </v-btn>\n\n            </v-form>\n        </div>\n\n        <!-- <div class=\"text-center\" max-width=\"1080px\" max-height=\"450\">\n            <iframe src=\"https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d372175.96394708013!2d76.6639\n            7704362407!3d43.21736006703185!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x38836e7d16c5cbab%3A0x\n            3d44668fad986d76!2sAlmaty!5e0!3m2!1sen!2skz!4v1643695197990!5m2!1sen!2skz\" loading=\"lazy\"></iframe>\n        </div> -->\n       \n    </div>\n</template>\n<script>\nexport default {\n    data(){\n        return{\n            contacts: [\n                { title: 'About Us', src: 'https://cdn.pixabay.com/photo/2022/01/07/07/13/chicago-6921297_960_720.jpg', flex: 12 },\n            ],\n            valid: true,\n            name: '',\n            nameRules: [\n                v => !!v || 'Name is required',\n                v => (v && v.length <= 10) || 'Name must be less than 10 characters',\n            ],\n            email: '',\n            emailRules: [\n                v => !!v || 'E-mail is required',\n                v => /.+@.+\\..+/.test(v) || 'E-mail must be valid',\n            ],\n            message: '',\n            messageRules: [\n                v => !!v || 'Message is required',\n                v => (v && v.length >= 10) || 'Message must be more than 10 characters',\n            ],\n        }\n    },\n    methods: {\n      validate () {\n        this.$refs.form.validate()\n      },\n      reset () {\n        this.$refs.form.reset()\n      },\n    },\n}\n</script>"],"sourceRoot":"src/views"}]}